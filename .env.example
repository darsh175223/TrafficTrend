# =============================================================================
# TrafficTrend - Environment Variables Template
# =============================================================================
# COPY THIS TO .env AND FILL IN THE VALUES
# DO NOT COMMIT THE .env FILE TO GITHUB
# =============================================================================

# AWS Configuration
AWS_REGION=us-west-2

# Terraform Secrets (Terraform automatically reads TF_VAR_ prefix)
TF_VAR_db_username=admin
TF_VAR_db_password=CHANGE_ME_secure_password_123!
TF_VAR_jwt_secret=CHANGE_ME_your_super_secret_jwt_key_at_least_32_characters_long!

# App Settings
ENVIRONMENT=dev
PROJECT_NAME=traffictrend
